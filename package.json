{
  "name": "shieldbattery",
  "private": true,
  "version": "5.0.4",
  "description": "The modern way to play StarCraft: Brood War.",
  "homepage": "https://github.com/tec27/shieldbattery#readme",
  "main": "index.js",
  "engines": {
    "node": ">= 7.0.0"
  },
  "scripts": {
    "app": "cross-env SB_HOT=1 electron app",
    "autotest": "cross-env BABEL_ENV=node mocha --watch -R spec --compilers js:babel-register --recursive --harmony test/",
    "build": "cross-env BABEL_ENV=node NODE_ENV=production webpack",
    "buildgame": "game/vcbuild.bat && cd game && yarn run build",
    "clean": "rimraf app/dist && rimraf app/styles",
    "dist": "yarn run clean && yarn run build && yarn run buildgame && build --win --ia32 --x64 -p always -c prod.yml",
    "diststaging": "yarn run clean && cross-env SB_SERVER=https://dev.shieldbattery.net yarn run build && yarn run buildgame && build --win --ia32 --x64 -p always -c staging.yml",
    "dev": "cross-env BABEL_ENV=node node -r babel-register electron-dev-server.js",
    "lint": "eslint --ext .js,.jsx ./ && echo Hooray || echo Please fix",
    "pack": "yarn run clean && yarn run build && yarn run buildgame && build --dir --win --ia32 --x64 -c prod.yml",
    "packstaging": "yarn run clean && cross-env SB_SERVER=https://dev.shieldbattery.net yarn run build && yarn run buildgame && build --dir --win --ia32 --x64 -c staging.yml",
    "postinstall": "cross-env-test SB_SERVER_ONLY=1 || install-app-deps",
    "posttest": "npm run lint",
    "test": "cross-env BABEL_ENV=node mocha -R spec --compilers js:babel-register --recursive --harmony test/",
    "yarnall": "cd app && yarn && cd ../game && yarn && cd ../server && yarn && cd .. && yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tec27/shieldbattery.git"
  },
  "keywords": [
    "brood war",
    "shieldbattery",
    "starcraft"
  ],
  "author": "Travis Collins <travis@tec27.com> (http://tec27.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tec27/shieldbattery/issues"
  },
  "dependencies": {
    "@exponent/electron-cookies": "^2.0.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.1.8",
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "cross-env-test": "^0.1.1",
    "cuid": "^1.3.8",
    "electron-is-dev": "^0.1.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "history": "^2.1.2",
    "immutable": "^3.8.1",
    "jssuh": "^1.1.0",
    "keycode": "^2.1.8",
    "mkdirp": "^0.5.1",
    "nydus": "^0.3.5",
    "nydus-client": "^0.3.1",
    "postcss-cssnext": "^2.9.0",
    "postcss-for": "^2.1.1",
    "postcss-mixins": "^5.4.1",
    "rally-point-player": "^1.0.0",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-custom-scrollbars": "^4.0.0",
    "react-dom": "^15.4.1",
    "react-ga": "^2.1.2",
    "react-redux": "^5.0.1",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "react-svg-loader": "2.0.0-alpha.1",
    "redux": "^3.6.0",
    "redux-batched-subscribe": "^0.1.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "thenify": "^3.2.1",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.27.3",
    "electron": "^1.4.13",
    "electron-builder": "latest",
    "electron-devtools-installer": "^2.0.1",
    "eslint": "^3.12.2",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "html-loader": "^0.4.4",
    "markdown-loader": "latest",
    "mocha": "^3.2.0",
    "postcss-loader": "^1.2.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.0",
    "webpack": "latest",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0"
  }
}
