{
  "name": "shieldbattery",
  "private": true,
  "version": "6.1.3",
  "description": "The modern way to play StarCraft: Brood War.",
  "homepage": "https://github.com/tec27/shieldbattery#readme",
  "main": "index.js",
  "engines": {
    "node": ">= 7.0.0"
  },
  "scripts": {
    "app": "cross-env SB_HOT=1 electron app",
    "autotest": "cross-env BABEL_ENV=node mocha --watch --watch-extensions js -R spec --require @babel/register --recursive --harmony test/",
    "build": "cross-env BABEL_ENV=node NODE_ENV=production webpack",
    "buildgame": "game/build.bat release sign",
    "clean": "rimraf app/dist && rimraf app/styles",
    "dist": "yarn run clean && yarn run build && yarn run buildgame && electron-builder build --win --ia32 --x64 -p always -c prod.yml",
    "diststaging": "yarn run clean && cross-env SB_SERVER=https://dev.shieldbattery.net yarn run build && yarn run buildgame && electron-builder build --win --ia32 --x64 -p always -c staging.yml",
    "dev": "cross-env BABEL_ENV=node node -r @babel/register electron-dev-server.js",
    "deploy": "yarn run dist && cross-env-shell deploy.bat $npm_package_version",
    "deploystaging": "yarn run diststaging && cross-env-shell deploy.bat $npm_package_version",
    "lint": "eslint --ext .js,.jsx ./",
    "pack": "yarn run clean && yarn run build && yarn run buildgame && electron-builder build --dir --win --ia32 --x64 -c prod.yml",
    "packstaging": "yarn run clean && cross-env SB_SERVER=https://dev.shieldbattery.net yarn run build && yarn run buildgame && electron-builder build --dir --win --ia32 --x64 -c staging.yml",
    "postinstall": "cross-env-test SB_SERVER_ONLY=1 || electron-builder install-app-deps",
    "posttest": "yarn run lint",
    "test": "cross-env BABEL_ENV=node mocha -R spec --require @babel/register --recursive --harmony test/ && cd server && yarn test",
    "yarnall": "cd app && yarn && cd ../server && yarn && cd .. && yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tec27/shieldbattery.git"
  },
  "keywords": [
    "brood war",
    "shieldbattery",
    "starcraft"
  ],
  "author": "Travis Collins <travis@tec27.com> (http://tec27.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tec27/shieldbattery/issues"
  },
  "dependencies": {
    "@exponent/electron-cookies": "^2.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "bw-chk": "^1.1.0",
    "classnames": "^2.2.6",
    "concat-stream": "^2.0.0",
    "connected-react-router": "^6.6.1",
    "core-js": "^3.6.1",
    "cross-env-test": "^0.1.1",
    "cuid": "^2.1.8",
    "deep-equal": "^2.0.1",
    "electron-is-dev": "^1.2.0",
    "filereader-stream": "^2.0.0",
    "glob": "^7.1.6",
    "history": "^4.10.1",
    "immutable": "^4.0.0-rc.12",
    "jssuh": "^1.4.0",
    "keycode": "^2.2.0",
    "lodash.debounce": "^4.0.8",
    "memoize-one": "^5.1.1",
    "mkdirp": "^1.0.4",
    "nydus": "^1.0.4",
    "nydus-client": "^1.0.2",
    "polished": "^3.4.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-for": "^2.1.1",
    "postcss-mixins": "^6.2.3",
    "prop-types": "^15.7.2",
    "query-string": "^6.9.0",
    "rally-point-player": "^1.0.0",
    "react": "^16.12.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-hot-loader": "^4.12.18",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-svg-loader": "3.0.3",
    "react-transition-group": "^4.3.0",
    "react-virtualized": "^9.21.2",
    "redux": "^4.0.5",
    "redux-batched-subscribe": "^0.1.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5",
    "request": "^2.88.0",
    "scm-extractor": "^1.0.6",
    "styled-components": "^5.1.0",
    "thenify": "^3.2.1",
    "whatwg-fetch": "^3.0.0",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
