    "immutable": "^3.8.1",
    "jssuh": "^1.1.0",
    "keycode": "^2.1.8",
    "mkdirp": "^0.5.1",
    "node-interval-tree": "^1.0.0",
    "nydus": "^1.0.2",
    "nydus-client": "^1.0.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-for": "^2.1.1",
    "postcss-mixins": "^6.0.0",
    "prop-types": "^15.5.10",
    "rally-point-player": "^1.0.0",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-custom-scrollbars": "^4.0.0",
    "react-dom": "^15.4.1",
    "react-ga": "^2.1.2",
