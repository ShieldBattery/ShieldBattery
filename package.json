  "engines": {
    "node": ">= 22.12.0"
  },
  "scripts": {
    "app": "SB_HOT=1 electron app",
    "build-app-client": "NODE_ENV=production webpack",
    "build-app-client-stats": "NODE_ENV=production webpack --profile --json > stats.json",
    "build-game": "cmd /c \"game\\build.bat release\" && cmd /c \"game\\build.bat release x86_64\"",
    "build-web-client": "NODE_ENV=production webpack --config ./server/webpack.config.js",
    "build-web-client-stats": "NODE_ENV=production webpack --profile --json --config ./server/webpack.config.js > stats.json",
    "check-circular": "madge --circular --extensions ts,tsx,js,jsx ./app ./client ./server",
    "check-orphans": "madge --orphans --extensions ts,tsx,js,jsx --exclude \"/dist/|[.]test[.]|/migrations/|/testing/\" ./app ./client ./server",
    "clean": "rimraf app/dist && rimraf app/styles",
    "dev": "node -r ./babel-register electron-dev-server.js",
    "dist": "pnpm run clean && pnpm run build-app-client && pnpm run build-game && electron-builder build --win --x64 -p always -c prod.yml",
    "diststaging": "pnpm run clean && SB_SERVER=https://staging.shieldbattery.net pnpm run build-app-client && pnpm run build-game && electron-builder build --win --x64 -p always -c staging.yml",
    "download-fonts": "node -r ./babel-register download-fonts.ts",
    "email-dev": "email dev --dir email/templates --port 5567",
    "gen-emails": "node -r ./babel-register email/generate.tsx",
    "gen-graphql": "graphql-codegen --config graphql-codegen.ts",
    "gen-schema": "cd ./server-rs && cargo run --bin gen-schema",
    "gen-translations": "pnpm gen-translations:app && pnpm gen-translations:email",
    "gen-translations:app": "i18next",
    "gen-translations:email": "i18next -c email/i18next-parser.config.json",
    "gen-typeshare": "typeshare ./server-rs --lang=typescript --output-file=common/typeshare.ts && prettier --write common/typeshare.ts",
    "installall": "cd app && pnpm install && cd .. && pnpm install",
    "lint": "eslint .",
    "local-dev": "run-pty local-dev.json",
    "migrate:create": "cargo sqlx migrate add",
    "migrate:run": "cargo sqlx migrate run",
    "pack": "pnpm run clean && pnpm run build-app-client && pnpm run build-game && electron-builder build --dir --win --x64 -c prod.yml",
    "packstaging": "pnpm run clean && SB_SERVER=https://staging.shieldbattery.net pnpm run build-app-client && pnpm run build-game && electron-builder build --dir --win --x64 -c staging.yml",
    "sqlx-prepare": "[ \"${NODE_ENV:-}\" != \"production\" ] && (cd ./server-rs && cargo sqlx prepare --workspace -- --all-targets --all-features) || echo \"In production, not sqlx-preparing\"",
    "start-server": "node --trace-warnings ./server/index.js | pino-pretty -c -t -i pid,hostname,req.headers,res.headers,req.remoteAddress,req.remotePort",
    "test": "vitest",
    "test-ui": "pnpm test --ui --coverage.enabled=true",
    "test:integration": "NODE_ENV=production playwright test",
    "typecheck": "tsc --skipLibCheck false"
  },
