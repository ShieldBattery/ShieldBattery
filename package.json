  "engines": {
    "node": ">= 12.0.0"
  },
  "scripts": {
    "app": "cross-env SB_HOT=1 electron app",
    "autotest": "cross-env BABEL_ENV=node mocha --watch --watch-extensions js -R spec --require ./babel-register --extension js,ts --recursive test/",
    "build-app-client": "cross-env BABEL_ENV=node NODE_ENV=production webpack",
    "build-app-client-stats": "cross-env BABEL_ENV=node NODE_ENV=production webpack --profile --json > stats.json",
    "build-game": "game/build.bat release sign",
    "build-web-client": "cross-env BABEL_ENV=node NODE_ENV=production webpack --config ./server/webpack.config.js",
    "build-web-client-stats": "cross-env BABEL_ENV=node NODE_ENV=production webpack --profile --json --config ./server/webpack.config.js > stats.json",
    "clean": "rimraf app/dist && rimraf app/styles",
    "dev": "cross-env BABEL_ENV=node node -r ./babel-register electron-dev-server.js",
    "dist": "yarn run clean && yarn run build-app-client && yarn run build-game && electron-builder build --win --ia32 --x64 -p always -c prod.yml",
    "diststaging": "yarn run clean && cross-env SB_SERVER=https://staging.shieldbattery.net yarn run build-app-client && yarn run build-game && electron-builder build --win --ia32 --x64 -p always -c staging.yml",
    "postinstall": "cross-env-test SB_SERVER_ONLY=1 || electron-builder install-app-deps",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "migrate-create": "db-migrate -m ./server/migrations create",
    "migrate-down": "db-migrate -m ./server/migrations down",
    "migrate-up": "db-migrate -m ./server/migrations up",
    "pack": "yarn run clean && yarn run build-app-client && yarn run build-game && electron-builder build --dir --win --ia32 --x64 -c prod.yml",
    "packstaging": "yarn run clean && cross-env SB_SERVER=https://staging.shieldbattery.net yarn run build-app-client && yarn run build-game && electron-builder build --dir --win --ia32 --x64 -c staging.yml",
    "start-server": "node ./server/index.js | pino-pretty -c -t",
    "test": "cross-env BABEL_ENV=node mocha -R spec --require ./babel-register --extension js,ts --recursive test/",
    "typecheck": "tsc",
    "posttest": "yarn run typecheck && yarn run lint",
    "yarnall": "cd app && yarn && cd .. && yarn"
  },
