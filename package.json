  "author": "Travis Collins <travis@tec27.com> (http://tec27.com/)",
  "main": "index.js",
  "engines": {
    "node": ">= 14.0.0"
  },
  "jest": {
    "projects": [
      {
        "displayName": "node",
        "setupFilesAfterEnv": [
          "./babel-register.js",
          "core-js/proposals/reflect-metadata",
          "jest-extended/all"
        ],
        "testEnvironment": "node",
        "testMatch": [
          "<rootDir>/app/**/*.test.[jt]s?(x)",
          "<rootDir>/common/**/*.test.[jt]s?(x)",
          "<rootDir>/server/**/*.test.[jt]s?(x)"
        ]
      },
      {
        "displayName": "browser",
        "moduleNameMapper": {
          "\\.svg$": "<rootDir>/client/__mocks__/svg-mock.tsx",
          "\\.(html|htm|md)$": "<rootDir>/client/__mocks__/static-file-mock.ts"
        },
        "setupFilesAfterEnv": [
          "./babel-register.js",
          "core-js/proposals/reflect-metadata",
          "jest-extended/all",
          "./jest-client-setup.ts"
        ],
        "testEnvironment": "jsdom",
        "testMatch": [
          "<rootDir>/client/**/*.test.[jt]s?(x)"
        ]
      }
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/app/dist",
      "<rootDir>/app/node_modules",
      "<rootDir>/dist",
      "<rootDir>/node_modules",
      "<rootDir>/server/public"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/app/dist",
      "<rootDir>/app/node_modules",
      "<rootDir>/dist",
      "<rootDir>/node_modules",
      "<rootDir>/server/public"
    ]
  },
  "scripts": {
    "app": "cross-env SB_HOT=1 electron app",
    "build-app-client": "cross-env NODE_ENV=production webpack",
    "build-app-client-stats": "cross-env NODE_ENV=production webpack --profile --json > stats.json",
    "build-game": "game/build.bat release sign",
    "build-web-client": "cross-env NODE_ENV=production webpack --config ./server/webpack.config.js",
    "build-web-client-stats": "cross-env NODE_ENV=production webpack --profile --json --config ./server/webpack.config.js > stats.json",
    "clean": "rimraf app/dist && rimraf app/styles",
    "dev": "node -r ./babel-register electron-dev-server.js",
    "dist": "yarn run clean && yarn run build-app-client && yarn run build-game && electron-builder build --win --x64 -p always -c prod.yml",
    "diststaging": "yarn run clean && cross-env SB_SERVER=https://staging.shieldbattery.net yarn run build-app-client && yarn run build-game && electron-builder build --win --x64 -p always -c staging.yml",
    "postinstall": "cross-env-test SB_SERVER_ONLY=1 || electron-builder install-app-deps",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "migrate-create": "db-migrate -m ./server/migrations create",
    "migrate-down": "db-migrate -m ./server/migrations down",
    "migrate-up": "db-migrate -m ./server/migrations up",
    "pack": "yarn run clean && yarn run build-app-client && yarn run build-game && electron-builder build --dir --win --x64 -c prod.yml",
    "packstaging": "yarn run clean && cross-env SB_SERVER=https://staging.shieldbattery.net yarn run build-app-client && yarn run build-game && electron-builder build --dir --win --x64 -c staging.yml",
    "start-server": "node ./server/index.js | node ./server/utils/pino-pg | pino-pretty -c -t",
    "test": "jest",
    "testonly": "jest",
    "typecheck": "tsc --skipLibCheck false",
    "posttest": "yarn run typecheck && yarn run lint",
    "yarnall": "cd app && yarn && cd .. && yarn"
  },
