  "engines": {
    "node": ">= 20.0.0"
  },
  "jest": {
    "projects": [
      {
        "prettierPath": null,
        "displayName": "node",
        "setupFilesAfterEnv": [
          "./babel-register.js",
          "core-js/proposals/reflect-metadata",
          "jest-extended/all"
        ],
        "testEnvironment": "node",
        "testMatch": [
          "<rootDir>/app/**/*.test.[jt]s?(x)",
          "<rootDir>/common/**/*.test.[jt]s?(x)",
          "<rootDir>/server/**/*.test.[jt]s?(x)"
        ]
      },
      {
        "prettierPath": null,
        "displayName": "browser",
        "moduleNameMapper": {
          "\\.svg$": "<rootDir>/client/__mocks__/svg-mock.tsx",
          "\\.(html|htm|md)$": "<rootDir>/client/__mocks__/static-file-mock.ts"
        },
        "setupFilesAfterEnv": [
          "./babel-register.js",
          "core-js/proposals/reflect-metadata",
          "jest-extended/all",
          "./jest-client-setup.ts"
        ],
        "testEnvironment": "jsdom",
        "testMatch": [
          "<rootDir>/client/**/*.test.[jt]s?(x)"
        ]
      }
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/app/dist",
      "<rootDir>/app/node_modules",
      "<rootDir>/dist",
      "<rootDir>/node_modules",
      "<rootDir>/server/public"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/app/dist",
      "<rootDir>/app/node_modules",
      "<rootDir>/dist",
      "<rootDir>/node_modules",
      "<rootDir>/server/public"
    ]
  },
  "scripts": {
    "app": "cross-env SB_HOT=1 electron app",
    "build-app-client": "cross-env NODE_ENV=production webpack",
    "build-app-client-stats": "cross-env NODE_ENV=production webpack --profile --json > stats.json",
    "build-game": "game\\build.bat release",
    "build-web-client": "cross-env NODE_ENV=production webpack --config ./server/webpack.config.js",
    "build-web-client-stats": "cross-env NODE_ENV=production webpack --profile --json --config ./server/webpack.config.js > stats.json",
    "clean": "rimraf app/dist && rimraf app/styles",
    "dev": "node -r ./babel-register electron-dev-server.js",
    "dist": "pnpm run clean && pnpm run build-app-client && pnpm run build-game && electron-builder build --win --x64 -p always -c prod.yml",
    "diststaging": "pnpm run clean && cross-env SB_SERVER=https://staging.shieldbattery.net pnpm run build-app-client && pnpm run build-game && electron-builder build --win --x64 -p always -c staging.yml",
    "download-fonts": "node -r ./babel-register download-fonts.ts",
    "email-dev": "email dev --dir email/templates --port 5567",
    "gen-emails": "node -r ./babel-register email/generate.tsx",
    "gen-graphql": "graphql-codegen --config graphql-codegen.ts",
    "gen-schema": "cd ./server-rs && cargo run --bin gen-schema",
    "gen-translations": "pnpm gen-translations:app && pnpm gen-translations:email",
    "gen-translations:app": "i18next",
    "gen-translations:email": "i18next -c email/i18next-parser.config.json",
    "gen-typeshare": "typeshare ./server-rs --lang=typescript --output-file=common/typeshare.ts && prettier --write common/typeshare.ts",
    "installall": "cd app && pnpm install && cd .. && pnpm install",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "local-dev": "run-pty local-dev.json",
    "migrate-create": "db-migrate -m ./server/migrations create",
    "migrate-down": "db-migrate -m ./server/migrations down && pnpm run sqlx-prepare",
    "migrate-up": "db-migrate -m ./server/migrations up && pnpm run sqlx-prepare",
    "pack": "pnpm run clean && pnpm run build-app-client && pnpm run build-game && electron-builder build --dir --win --x64 -c prod.yml",
    "packstaging": "pnpm run clean && cross-env SB_SERVER=https://staging.shieldbattery.net pnpm run build-app-client && pnpm run build-game && electron-builder build --dir --win --x64 -c staging.yml",
    "sqlx-prepare": "cross-env-test NODE_ENV!=production && cd ./server-rs && cargo sqlx prepare --workspace -- --all-targets --all-features || echo In production, not sqlx-preparing",
    "start-server": "node ./server/index.js | pino-pretty -c -t",
    "test": "jest",
    "test:integration": "cross-env NODE_ENV=production playwright test",
    "typecheck": "tsc --skipLibCheck false"
  },
